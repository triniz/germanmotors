<p id="notice"><%= notice %></p>

<h1>Produits</h1>

<table>
  <thead>
    <tr>
      <th>Titre</th>
      <th>Description</th>
      <th>Prix</th>
      <th>Type</th>
      <th>Etat</th>
      <th>Kilometrage</th>
      <th>Cylindre</th>
      <th>Code</th>
      <th>Puissance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @produits.each do |produit| %>
      <tr>
        <td><%= produit.titre %></td>
        <td><%= produit.description %></td>
        <td><%= produit.prix %></td>
        <td><%= produit.type_produit %></td>
        <td><%= produit.etat %></td>
        <td><%= produit.kilometrage %></td>
        <td><%= produit.cylindre %></td>
        <td><%= produit.code %></td>
        <td><%= produit.puissance %></td>
        <td><%= link_to 'Show', produit %></td>
        <td><%= link_to 'Edit', edit_produit_path(produit) %></td>
        <td><%= link_to 'Destroy', produit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Produit', new_produit_path %>
